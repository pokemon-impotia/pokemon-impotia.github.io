      "abilities_data": [
        {
          "symbol": "ice_body",
          "textId": 115,
          "names": {
            "en": "Ice Body",
            "fr": "Corps Gel",
            "it": "Corpogelo",
            "de": "Eishaut",
            "es": "Gélido",
            "ko": "아이스바디"
          },
          "descriptions": {
            "en": "The Pokémon gradually regains HP in a hailstorm.",
            "fr": "Régénère les PV du Pokémon lors des tempêtes de grêle.",
            "it": "Se grandina, il Pokémon recupera PS.",
            "de": "Regeneriert bei Hagel nach und nach KP.",
            "es": "Recupera PS de forma gradual cuando hay tormentas de granizo.",
            "ko": "날씨가 싸라기눈일 때 HP를 조금씩 회복한다."
          }
        },
        {
          "symbol": "snow_warning",
          "textId": 117,
          "names": {
            "en": "Snow Warning",
            "fr": "Alerte Neige",
            "it": "Scendineve",
            "de": "Hagelalarm",
            "es": "Nevada",
            "ko": "눈퍼뜨리기"
          },
          "descriptions": {
            "en": "The Pokémon summons a hailstorm when it enters a battle.",
            "fr": "Déclenche une tempête de grêle quand le Pokémon entre au combat.",
            "it": "Quando il Pokémon entra in campo, provoca una grandinata.",
            "de": "Löst bei Kampfantritt Hagel aus.",
            "es": "Invoca una tormenta de granizo al entrar en combate.",
            "ko": "등장했을 때 날씨를 싸라기눈으로 만든다."
          }
        },
        {
          "symbol": "sturdy",
          "textId": 5,
          "names": {
            "en": "Sturdy",
            "fr": "Fermeté",
            "it": "Vigore",
            "de": "Robustheit",
            "es": "Robustez",
            "ko": "옹골참"
          },
          "descriptions": {
            "en": "It cannot be knocked out with one hit. One-hit KO moves cannot knock it out, either.",
            "fr": "Le Pokémon encaisse toujours au moins une attaque s’il a tous ses PV. Il est également immunisé contre les capacités pouvant mettre K.O. en un coup.",
            "it": "Evita che il Pokémon vada KO in un sol colpo se ha tutti i PS, e lo rende immune alle mosse che causano KO immediato.",
            "de": "Bietet Schutz gegen K.O.-Attacken. Bei vollen KP übersteht das Pokémon auch K.O.-Treffer.",
            "es": "Evita que el rival pueda debilitarlo de un solo golpe cuando tiene los PS al máximo. También evita los movimientos fulminantes.",
            "ko": "상대 기술을 받아도 일격으로 쓰러지지 않는다. 일격필살 기술도 효과 없다."
          }
        }
