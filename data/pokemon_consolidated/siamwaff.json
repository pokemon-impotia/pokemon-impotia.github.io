      "abilities_data": [
        {
          "symbol": "intimidate",
          "textId": 22,
          "names": {
            "en": "Intimidate",
            "fr": "Intimidation",
            "it": "Prepotenza",
            "de": "Bedroher",
            "es": "Intimidación",
            "ko": "위협"
          },
          "descriptions": {
            "en": "The Pokémon intimidates opposing Pokémon upon entering battle, lowering their Attack stat.",
            "fr": "Le Pokémon rugit lorsqu’il arrive au combat, ce qui intimide l’ennemi et baisse son Attaque.",
            "it": "Quando il Pokémon entra in campo, la sua prepotenza intimidisce i nemici, riducendone l’Attacco.",
            "de": "Senkt den Angriff der Gegner, indem es sie gleich zu Kampfantritt bedroht und einschüchtert.",
            "es": "Al entrar en combate amilana al rival de tal manera que su Ataque disminuye.",
            "ko": "등장했을 때 위협해서 상대를 위축시켜 상대의 공격을 떨어뜨린다."
          }
        },
        {
          "symbol": "bulletproof",
          "textId": 171,
          "names": {
            "en": "Bulletproof",
            "fr": "Pare-Balles",
            "it": "Antiproiettile",
            "de": "Kugelsicher",
            "es": "Antibalas",
            "ko": "방탄"
          },
          "descriptions": {
            "en": "Protects the Pokémon from some ball and bomb moves.",
            "fr": "Protège de certaines capacités lançant des projectiles comme des bombes et des balles.",
            "it": "Protegge da alcune mosse a base di proiettili e bombe.",
            "de": "Kann das Pokémon vor geworfenen kugelförmigen Objekten, wie zum Beispiel Bomben, schützen.",
            "es": "No le afectan las bombas ni algunos proyectiles.",
            "ko": "상대 구슬이나 폭탄 등 기술을 막을 수 있다."
          }
        },
        {
          "symbol": "parental_bond",
          "textId": 185,
          "names": {
            "en": "Parental Bond",
            "fr": "Amour Filial",
            "it": "Amorefiliale",
            "de": "Familienbande",
            "es": "Amor Filial",
            "ko": "부자유친"
          },
          "descriptions": {
            "en": "Parent and child each attacks.",
            "fr": "Permet à la mère et à son petit d’attaquer tous les deux en même temps.",
            "it": "Il Pokémon e il suo piccolo attaccano insieme.",
            "de": "Zwei Generationen setzen jeweils ein Mal zum Angriff an.",
            "es": "Une fuerzas con su cría y ataca dos veces.",
            "ko": "부모와 자식 2마리로 2번 공격할 수 있다."
          }
        }
      ],
